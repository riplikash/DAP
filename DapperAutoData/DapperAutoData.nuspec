		<license type="expression">MIT</license>
		<dependencies>
			<group targetFramework="net6.0">
				<!-- Add dependencies for net6.0 if needed -->
			</group>
			<group targetFramework="net7.0">
				<!-- Add dependencies for net7.0 if needed -->
			</group>
		</dependencies>
